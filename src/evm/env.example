
NETWORK=sepolia

PRIVATE_KEY=your_deployer_private_key_here

# ICP Network Signer Address (the address that will be authorized to create escrows)
# This should be the address of your ICP canister or a designated signer
ICP_NETWORK_SIGNER_ADDRESS=0x0000000000000000000000000000000000000000

# Rescue delay for source chain escrows (in seconds)
# Default: 1 day (86400 seconds)
RESCUE_DELAY_SRC=86400

# Rescue delay for destination chain escrows (in seconds)
# Default: 1 day (86400 seconds)
RESCUE_DELAY_DST=86400

# Sepolia RPC URL (if using custom RPC)
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Etherlink Testnet RPC URL (if using custom RPC)
# ETHERLINK_TESTNET_RPC_URL=https://node.ghostnet.tezos.marigold.dev

# Etherlink Mainnet RPC URL (if using custom RPC)
# ETHERLINK_MAINNET_RPC_URL=https://node.mainnet.tezos.marigold.dev

# Etherscan API Key (for Sepolia verification)
# ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Etherlink Explorer API Key (if available)
# ETHERLINK_EXPLORER_API_KEY=your_etherlink_explorer_api_key_here

# Gas price in wei (if you want to set a specific gas price)
# GAS_PRICE=20000000000

# Gas limit for deployment (if you want to set a specific gas limit)
# GAS_LIMIT=5000000

# Whether to verify contracts after deployment (true/false)
# VERIFY_CONTRACTS=true

# Whether to run tests after deployment (true/false)
# RUN_TESTS=true

# For Sepolia deployment:
# NETWORK=sepolia
# PRIVATE_KEY=your_sepolia_deployer_private_key
# ICP_NETWORK_SIGNER_ADDRESS=0x1234567890123456789012345678901234567890
# ETHERSCAN_API_KEY=your_etherscan_api_key

# For Etherlink Testnet deployment:
# NETWORK=etherlinkTestnet
# PRIVATE_KEY=your_etherlink_testnet_deployer_private_key
# ICP_NETWORK_SIGNER_ADDRESS=0x1234567890123456789012345678901234567890

# For Etherlink Mainnet deployment:
# NETWORK=etherlinkMainnet
# PRIVATE_KEY=your_etherlink_mainnet_deployer_private_key
# ICP_NETWORK_SIGNER_ADDRESS=0x1234567890123456789012345678901234567890
# VERIFY_CONTRACTS=true 